    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "usehooks-ts": "^2.9.1"
  }
}
